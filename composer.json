{
  "require": {
    "jarednova/timber": "*",
    "drewwestphal/CCWPOptions": "dev-master",
    "webdevstudios/cmb2": "2.0.*",
    "composer/installers": "~1.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/drewwestphal/CCWPOptions"
    }
  ],
  "autoload": {
    "classmap": [
      "vendor/plugins/timber"
    ],
    "files": [
      "vendor/plugins/cmb2/init.php"
    ]
  },
  "extra": {
    "installer-paths": {
      "vendor/plugins/{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  }
}
 