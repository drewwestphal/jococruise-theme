            "name": "exp_shadow_cruise_header",
            "type": "text",
            "instructions": "split lines with pipe (|)",
            "required": 0,
            "conditional_logic": 0,
            "wrapper": {
                "width": "",
                "class": "",
                "id": ""
            },
            "default_value": "Our Amazing Community | \"The Shadow Cruise\"",
            "placeholder": "",
